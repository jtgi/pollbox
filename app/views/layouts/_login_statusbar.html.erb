<div class="row">
	<div id="user_nav">
		<% if user_signed_in? %>
			Signed in as <%= current_user.email %>. Not you?
			<%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
		<% else %>
			<div class="span6"><%= link_to "Sign Up", new_user_registration_path %> or <%= link_to "Sign In", new_user_session_path %></div>
		<% end %>
	</div>
</div>
